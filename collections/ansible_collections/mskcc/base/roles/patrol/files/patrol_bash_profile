# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
  . ~/.bashrc
fi

# User specific environment and startup programs


# Set up the Patrol Variables
export PATROL_ADMIN=patrol;
export PATROL_HOME=/opt/patrol/PatrolAgent;
export PATH=$PATH:$PATROL_HOME:$PATROL_HOME/bin:/usr/local/bin;

if [ -f ~/.bash_profile_extra ]; then
        . ~/.bash_profile_extra
fi
